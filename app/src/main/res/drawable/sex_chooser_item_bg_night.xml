<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <stroke android:width="0.5dip" android:color="#ff000000" />
            <solid android:color="#ff242424" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="0.5dip" android:color="#ff000000" />
            <solid android:color="#ff242424" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="0.5dip" android:color="#ff000000" />
            <solid android:color="@color/more_action_dialog_bg_night" />
        </shape>
    </item>
</selector>